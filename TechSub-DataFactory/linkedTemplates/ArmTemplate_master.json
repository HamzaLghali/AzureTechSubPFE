{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "TechSub-DataFactory"
		},
		"AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"AzureDatabricks2_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks2'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://hamzalghstorageaccount.dfs.core.windows.net/"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0610-154357-4d6df2t8"
		},
		"AzureDatabricks2_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0610-154357-4d6df2t8"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://KeyTechSub.vault.azure.net/"
		},
		"SqlServerCon_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-AUM4LGI\\MSSQLSERVER01"
		},
		"SqlServerCon_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "techsub"
		},
		"SqlServerCon_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "hamza"
		},
		"SqlServerConnLS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-AUM4LGI\\MSSQLSERVER01"
		},
		"SqlServerConnLS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "techsub"
		},
		"SqlServerConnLS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "hamza"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "TechSub-DataFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_connectionString": {
						"value": "[parameters('AzureBlobStorage_connectionString')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"AzureDatabricks2_accessToken": {
						"value": "[parameters('AzureDatabricks2_accessToken')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"AzureDatabricks2_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks2_properties_typeProperties_existingClusterId')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"SqlServerCon_properties_typeProperties_server": {
						"value": "[parameters('SqlServerCon_properties_typeProperties_server')]"
					},
					"SqlServerCon_properties_typeProperties_database": {
						"value": "[parameters('SqlServerCon_properties_typeProperties_database')]"
					},
					"SqlServerCon_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerCon_properties_typeProperties_userName')]"
					},
					"SqlServerConnLS_properties_typeProperties_server": {
						"value": "[parameters('SqlServerConnLS_properties_typeProperties_server')]"
					},
					"SqlServerConnLS_properties_typeProperties_database": {
						"value": "[parameters('SqlServerConnLS_properties_typeProperties_database')]"
					},
					"SqlServerConnLS_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerConnLS_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}